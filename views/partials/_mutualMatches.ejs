<% Object.entries(matches).forEach(function([key, value]) { %>
  <div class="row">
    <div class="col">
      <h2><%= categoryNames[key] %></h2>
    </div>
  </div>
  <% if (value.length > 0) { %>
    <div class="row">
      <% value.forEach(function(match) { %>
        <div class="col-6 col-md-3 mb-4">
          <div class="card-container matches-container">
          <div class="card h-100" style="background-image: url('<%= match.picUrl %>');">
            <div class="card-body text-center">
              <h5 class="card-title"><%= match.name %></h5>
            </div>
          </div>
        </div>
      </div>
      <% }); %>
    </div>
  <% } else { %>
    <div class="row">
      <div class="col">
        <p>No matches found.</p>
      </div>
    </div>
  <% } %>
<% }); %>



